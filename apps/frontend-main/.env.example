PORT=80
REACT_APP__BACKEND_MAIN_API___BASEURL=/api # Proxied through our nginx to backend container. (See nginx config) 
REACT_APP__BACKEND_MAIN_STATIC___BASEURL=/backend-static # Proxied through our nginx to backend container. (See nginx config) 
