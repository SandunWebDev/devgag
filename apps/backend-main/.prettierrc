{
    "printWidth": 80,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always",
    "requirePragma": false,
    "proseWrap": "preserve",
    "endOfLine": "lf",
    "jsxBracketSameLine": true,
    "jsxSingleQuote": true
}
